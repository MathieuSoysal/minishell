#include "builtins.h"

void ft_variable(int start, int end)
{
    start = 0;
    end = 0;
}


char * get_export_variable_name(char *variable)
{
    int index;
    int start;
    int end;
    ft_variable(index, start);
    if(!variable)
        return(NULL);
    while(variable[index])
    {
        if(is_blank(variable[index]))
                return(NULL);
        else if (variable[index] ==  '=')
        {
             end =  index - 1;
             return(malloc_substrcpy(variable, start, end));
        }
        index ++;
    }
    end =  index;
        return malloc_substrcpy(variable, start, end);
      
}
